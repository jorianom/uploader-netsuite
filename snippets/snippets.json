{
    "NetSuite Restlet Template": {
        "prefix": "nrestlet",
        "body": [
            "/**",
            " * @NApiVersion 2.1",
            " * @NScriptType Restlet",
            " * Version    Date            Author                     Remarks",
            " * 1.00       $CURRENT_DATE/$CURRENT_MONTH/$CURRENT_YEAR    ${1:Author Name}           ${2:Initial Remarks}",
            " */",
            "define(['N/log', 'N/record'], function (log, record) {",
            "",
            "    function _post(context) {",
            "        try {",
            "            log.debug('Context', context);",
            "            return { success: true };",
            "        } catch (error) {",
            "            log.error('Error', error);",
            "            return { success: false };",
            "        }",
            "    }",
            "",
            "    return {",
            "        post: _post",
            "    };",
            "});"
        ],
        "description": "Crea una estructura básica de un Restlet en NetSuite con la fecha actual."
    },
    "NetSuite Suitelet Template": {
        "prefix": "nsuitelet",
        "body": [
            "/**",
            " * @NApiVersion 2.1",
            " * @NScriptType Suitelet",
            " * Version    Date            Author                     Remarks",
            " * 1.00       $CURRENT_DATE/$CURRENT_MONTH/$CURRENT_YEAR    ${1:Author Name}           ${2:Initial Remarks}",
            " */",
            "define(['N/log', 'N/ui/serverWidget'], function (log, serverWidget) {",
            "",
            "    function onRequest(context) {",
            "        if (context.request.method === 'GET') {",
            "            var form = serverWidget.createForm({ title: 'Sample Form' });",
            "            form.addSubmitButton({ label: 'Submit' });",
            "            context.response.writePage(form);",
            "        }",
            "    }",
            "",
            "    return {",
            "        onRequest: onRequest",
            "    };",
            "});"
        ],
        "description": "Crea una estructura básica de un Suitelet en NetSuite con la fecha actual."
    },
    "NetSuite Scheduled Script Template": {
        "prefix": "nscheduled",
        "body": [
            "/**",
            " * @NApiVersion 2.1",
            " * @NScriptType ScheduledScript",
            " * Version    Date            Author                     Remarks",
            " * 1.00       $CURRENT_DATE/$CURRENT_MONTH/$CURRENT_YEAR    ${1:Author Name}           ${2:Initial Remarks}",
            " */",
            "define(['N/log', 'N/record'], function (log, record) {",
            "",
            "    function execute(context) {",
            "        try {",
            "            log.debug('Scheduled Script Execution Started', context);",
            "            // Your scheduled script logic here",
            "        } catch (error) {",
            "            log.error('Error', error);",
            "        }",
            "    }",
            "",
            "    return {",
            "        execute: execute",
            "    };",
            "});"
        ],
        "description": "Crea una estructura básica de un Scheduled Script en NetSuite con la fecha actual."
    },
    "NetSuite Map/Reduce Script Template": {
        "prefix": "nmapreduce",
        "body": [
            "/**",
            " * @NApiVersion 2.1",
            " * @NScriptType MapReduceScript",
            " * Version    Date            Author                     Remarks",
            " * 1.00       $CURRENT_DATE/$CURRENT_MONTH/$CURRENT_YEAR    ${1:Author Name}           ${2:Initial Remarks}",
            " */",
            "define(['N/log', 'N/record', 'N/search'], function (log, record, search) {",
            "",
            "    function getInputData() {",
            "        log.debug('Map/Reduce Script - getInputData');",
            "        return search.create({",
            "            type: record.Type.CUSTOMER,",
            "            filters: [],",
            "            columns: []",
            "        });",
            "    }",
            "",
            "    function map(context) {",
            "        log.debug('Map/Reduce Script - map', context.key);",
            "        // Your map logic here",
            "    }",
            "",
            "    function reduce(context) {",
            "        log.debug('Map/Reduce Script - reduce', context.key);",
            "        // Your reduce logic here",
            "    }",
            "",
            "    function summarize(summary) {",
            "        log.debug('Map/Reduce Script - summarize', summary);",
            "        // Your summary logic here",
            "    }",
            "",
            "    return {",
            "        getInputData: getInputData,",
            "        map: map,",
            "        reduce: reduce,",
            "        summarize: summarize",
            "    };",
            "});"
        ],
        "description": "Crea una estructura básica de un Map/Reduce Script en NetSuite con la fecha actual."
    },
    "NetSuite NModuleScope Public Template": {
        "prefix": "npublic",
        "body": [
            "/**",
            " * @NApiVersion 2.1",
            " * @NModuleScope Public",
            " * Version    Date            Author                     Remarks",
            " * 1.00       $CURRENT_DATE/$CURRENT_MONTH/$CURRENT_YEAR     ${1:Author Name}           ${2:Initial Remarks}",
            " */",
            "define(['N/log', 'N/record'], function (log, record) {",
            "",
            "    function exampleFunction(param) {",
            "        log.debug('Example Function', param);",
            "        // Your logic here",
            "        return { message: 'This is a public module function' };",
            "    }",
            "",
            "    return {",
            "        exampleFunction: exampleFunction",
            "    };",
            "});"
        ],
        "description": "Crea una estructura básica de un módulo público en NetSuite con la fecha actual."
    }
}
{
  "name": "uploader-netsuite",
  "displayName": "uploader-netsuite",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Uploader NetSuite Settings",
      "properties": {
        "uploaderNetSuite.consumerKey": {
          "type": "string",
          "default": "",
          "description": "Tu API Key para conectar a NetSuite"
        },
        "uploaderNetSuite.consumerSecret": {
          "type": "string",
          "default": "",
          "description": "Tu API Secret para conectar a NetSuite"
        },
        "uploaderNetSuite.accessToken": {
          "type": "string",
          "default": "",
          "description": "Ruta del archivo a subir a NetSuite"
        },
        "uploaderNetSuite.tokenSecret": {
          "type": "string",
          "default": "",
          "description": "ID de la carpeta en NetSuite donde se subirá el archivo"
        },
        "uploaderNetSuite.urlScript": {
          "type": "string",
          "default": "",
          "description": "ID de la carpeta en NetSuite donde se subirá el archivo"
        },
        "uploaderNetSuite.realm": {
          "type": "string",
          "default": "",
          "description": "ID de la carpeta en NetSuite donde se subirá el archivo"
        }
      }
    },
    "commands": [
      {
        "command": "uploader-netsuite.Netsuite:Push File",
        "title": "Push File to NetSuite"
      },
      {
        "command": "uploader-netsuite.Netsuite:Pull File",
        "title": "Pull File from NetSuite"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "uploader-netsuite",
          "title": "Uploader Netsuite",
          "icon": "src/resources/upload.svg"
        }
      ]
    },
    "views": {
      "uploader-netsuite": [
        {
          "id": "uploader-netsuite",
          "name": "Uploader Netsuite"
        },
        {
          "id": "uploader-net",
          "name": "Uploader Net"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "uploader-netsuite",
        "contents": "Bienvenido, In order to use git features, you can open a folder containing a git repository or clone from a URL.\n[Open Folder](command:uploader-netsuite.helloWorld)\n[Clone Repository](command:git.clone)\nTo learn more about how to use git and source control in VS Code [read our docs](https://aka.ms/vscode-scm).",
        "when": "config.git.enabled && git.state == initialized && workbenchState == empty"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "crypto": "^1.0.1",
    "form-data": "^4.0.0",
    "oauth-1.0a": "^2.2.6"
  }
}